{
  "name": "frontend-new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "prepare": "husky install",
    "format-all": "prettier --write \"src/**/*.{js,ts,svelte}\" && eslint --fix \"src/**/*.{js,ts,svelte}\" && stylelint --fix \"src/**/*.{css,scss,less,styl}\""
  },
  "lint-staged": {
    "src/**/*.{js,ts,svelte}": [
      "prettier --write",
      "eslint --fix --config eslint.config.js"
    ],
    "src/**/*.{css,scss,less,styl}": [
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.14.1",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-svelte": "^3.5.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "svelte": "^5.23.1",
    "svelte-check": "^4.1.5",
    "svelte-eslint-parser": "^1.1.3",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vite-plugin-markdown": "^2.2.0"
  },
  "dependencies": {
    "@googleapis/drive": "^12.0.0",
    "axios": "^1.8.4",
    "chart.js": "^4.4.9",
    "feather-icons": "^4.29.2",
    "openai": "^4.96.0",
    "sql.js": "^1.8.0",
    "svelte-spa-router": "^4.0.1",
    "wa-sqlite": "^1.0.0"
  }
}
